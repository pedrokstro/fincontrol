{
  "name": "fincontrol",
  "version": "1.0.0",
  "description": "Sistema completo de controle financeiro pessoal com React, TypeScript e Tailwind CSS",
  "author": "Seu Nome <seu.email@exemplo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/fincontrol.git"
  },
  "homepage": "https://github.com/seu-usuario/fincontrol#readme",
  "bugs": {
    "url": "https://github.com/seu-usuario/fincontrol/issues"
  },
  "keywords": [
    "finance",
    "financial-control",
    "budget",
    "expenses",
    "income",
    "react",
    "typescript",
    "tailwindcss",
    "zustand",
    "recharts",
    "personal-finance",
    "money-management"
  ],
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "migrate": "node scripts/run-migrations.js",
    "migration:create": "node scripts/create-migration.js",
    "migration:status": "node scripts/migration-status.js",
    "db:backup": "node scripts/backup-database.js create",
    "db:restore": "node scripts/backup-database.js restore",
    "db:list": "node scripts/backup-database.js list",
    "db:test": "node scripts/test-connection.js",
    "deploy": "npm run build && npm run migrate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "emoji-picker-react": "^4.15.0",
    "framer-motion": "^12.23.24",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.10.3",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "pg": "^8.16.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
